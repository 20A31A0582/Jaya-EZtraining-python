#swap with temp variable
a=int(input())
b=int(input())
print("Before Swapping:")
print(a,b)
t=a
a=b
b=t
Print("After Swapping:")
print(a,b)

#swap without temp
a=int(input())
b=int(input())
print("Before Swapping:")
print(a,b)
a,b=b,a
Print("After Swapping:")
print(a,b)

#print perimeter and area of rectangle
a=int(input())
b=int(input())
print("Perimeter:")
print(2*(a+b))
print("Area:")
print(a*b)


